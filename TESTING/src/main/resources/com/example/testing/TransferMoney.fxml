<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ADD8E6;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testing.Controllers.TransferMoneyController">
    <children>
        <ToolBar prefHeight="40.0" prefWidth="605.0">
            <items>
                <Label fx:id="transferMoneyLabel" alignment="CENTER" prefHeight="26.0" prefWidth="98.0" text="Transfer Money" textAlignment="CENTER" textFill="#0b55b5">
                    <font>
                        <Font name="Bahnschrift Bold" size="14.0" />
                    </font>
                </Label>
            </items>
        </ToolBar>

        <Label fx:id="amountLBL" alignment="CENTER" contentDisplay="CENTER" layoutX="130.0" layoutY="151.0" prefHeight="26.0" prefWidth="95.0" text="AMOUNT" textAlignment="CENTER" textFill="#0b55b5" wrapText="true">
            <font>
                <Font name="Bahnschrift Bold" size="14.0" />
            </font>
        </Label>
        <Label fx:id="idLBL" alignment="CENTER" contentDisplay="CENTER" layoutX="130.0" layoutY="204.0" prefHeight="26.0" prefWidth="95.0" text="USERNAME" textAlignment="CENTER" textFill="#0b55b5" wrapText="true">
            <font>
                <Font name="Bahnschrift Bold" size="14.0" />
            </font>
        </Label>
        <TextField fx:id="amountTXT" layoutX="236.0" layoutY="151.0" prefHeight="26.0" prefWidth="176.0" />
        <TextField fx:id="username" layoutX="236.0" layoutY="204.0" prefHeight="26.0" prefWidth="176.0" />
        <Button fx:id="transferBTN" layoutX="265.0" layoutY="273.0" mnemonicParsing="false" onMouseClicked="#transfer" prefHeight="34.0" prefWidth="95.0" text="TRANSFER" textFill="#0b55b5">
            <font>
                <Font name="Bahnschrift Bold" size="14.0" />
            </font>
        </Button>
        <Button fx:id="backbuttonn4" layoutX="34.0" layoutY="349.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="26.0" prefWidth="60.0" text="Back" textFill="#0b55b5">
            <font>
                <Font name="Bahnschrift Bold" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
